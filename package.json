{
  "name": "@hypersignals/perp-folio",
  "version": "0.1.26",
  "description": "Multi-chain DEX position aggregator SDK with React hooks",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "defi",
    "dex",
    "perpetuals",
    "hyperliquid",
    "lighter",
    "pacifica",
    "positions",
    "react",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@hypersignals/dex-ws": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@hypersignals/dex-ws": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hypersignals/dex-ws": "^1.0.2",
    "@tanstack/react-query": "^5.62.8",
    "@types/react": "^18.3.12",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "viem": "^2.43.1"
  }
}
